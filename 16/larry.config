# NikGapps Config: "The Works" - Optimized for Larry (Voltage OS)
# Goal: Max Integration & Performance | Android 16
# Exclusions: No Go Apps, No Recorder, No Meet

AndroidVersion=16
Version=37

LogDirectory=default
InstallPartition=default
Mode=install
WipeDalvikCache=1
WipeRuntimePermissions=0
ExecuteBackupRestore=1
UseZipConfig=0
GmsOptimization=0
GenerateLogs=1

# --- THE CORE (Essential) ---
Core=1
>>ExtraFiles=1
>>GooglePlayStore=1
>>GoogleServicesFramework=1
>>GoogleContactsSyncAdapter=1
>>GoogleCalendarSyncAdapter=1
>>GmsCore=1

# --- MAX INTEGRATION (Replace AOSP Apps) ---
DigitalWellbeing=1
GoogleMessages=1
GoogleDialer=1
GoogleContacts=1
CarrierServices=1
GoogleClock=1
GoogleCalculator=1
GBoard=1
GoogleCalendar=1
GoogleKeep=1
GoogleChrome=1
>>WebViewGoogle=1
>>TrichromeLibrary=1

# --- SYSTEM & UTILITY ---
SetupWizard=1
>>SetupWizard=1
>>GoogleRestore=1
>>GoogleOneTimeInitializer=1
DeviceHealthServices=1
GoogleFiles=1
>>GoogleFiles=1
>>StorageManager=1
>>DocumentsUIGoogle=1
MarkupGoogle=1
GoogleTTS=1
DeviceSetup=1
AndroidAuto=1
GoogleFeedback=1
GooglePartnerSetup=1
AndroidDevicePolicy=1

# --- PIXEL EXCLUSIVES & EXPERIENCE ---
PixelSpecifics=1
>>PixelLauncher=1
>>DevicePersonalizationServices=1
>>GoogleWallpaper=1
>>QuickAccessWallet=1
>>SettingsServices=1
>>PrivateComputeServices=1
>>PixelThemes=1
>>EmojiWallpaper=1
>>PixelWeather=1
>>AICore=1
PixelSetupWizard=1
GoogleSounds=1
GooglePersonalSafety=1
GoogleTalkback=1

# --- MEDIA & CONTENT (Max Performance) ---
Drive=1
GoogleMaps=1
GoogleLocationHistory=1
GooglePhotos=1
GoogleSearch=1
>>Velvet=1
>>Assistant=1
PlayGames=1
YouTube=1
YouTubeMusic=1
